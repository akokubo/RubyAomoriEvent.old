<h1>Editing user</h1>

<% form_for(@user) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :nane %><br />
    <%= f.text_field :nane %>
  </p>
  <p>
    <%= f.label :passward %><br />
    <%= f.text_field :passward %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @user %> |
<%= link_to 'Back', users_path %>